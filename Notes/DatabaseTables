Inventory
	name text
	Range FK
	ShortDescription Text
	Color FK
	FullDescription Text
	Finish FK
	Category FK
	Size FK
	Picture photo
	Tags FK
	ReorderLevel number
	Max_selling_price number
	Min_selling_price number
	Wholesale_price number
	Wholesale_min_number number
	CurrentStock number
	InventoryCode text
	RecentBuyingPrice number
	Deactivate boolean
	DeactivationReason text

Promotion
	Inventory FK
	Promotion_Price amount
	Promotion_start_date date
	Promotion_end_date date
	Description text

DamagedInventory
	Inventory FK
	Count Number
	Reason Text
	Person FK
	QuantityBefore number
	QuantityAfter number
	Replacable boolean
	Supplier FK

Category
	name text
	parent FK
	image photo

Color
	name text
	category FK

Finish
	name text
	category FK

Size
	value decimal
	category FK
	type Choice_vol_length_weight

Range
	name text

Customers
	name string
	phone_number number
	isCredit boolean
	physical_address text
	Pin_number text
	VAT_number text
	email email

SalesOrder
	TotalAmount number
	SalesAgent FK
	Customer FK
	ReceiptNumber text (autogenerated)
	TransactionType Choice_Credit_Cash_Mpesa

SalesItems
	SalesOrder FK
	Inventory FK
	Quantity number
	TotalAmount number


CreditNote
	SalesItems FK (ManyToMany)
	Reason Text
	TotalAmount Number


Replenishment
	Inventory FK
	Supplier FK
	Count Number
	Price_per_item number
	ReceiptNumber text
	TotalAmount number

Suppliers
	Name Text
	phone_number_1 Number
	phone_number_2 Number
